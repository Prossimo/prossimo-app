@require('./partials/base-dialog-start-wrapper.hbs')
<div class="create-project-box">
    <form action="">
        <input name="project_name" type="text" required class="form-control" placeholder="Project name">
        <input name="client_name" type="text" class="form-control" placeholder="Client name">
        <input name="company" type="text" class="form-control" placeholder="Company">
        <input name="phone" type="text" class="form-control" placeholder="Phone">
        <input name="email" type="text" class="form-control" placeholder="E-mail">
        <input name="client_address" type="text" class="form-control" placeholder="Client address">
        <input name="project_address" type="text" class="form-control" placeholder="Project address">
        <textarea name="project_notes" class="form-control" placeholder="Project notes"></textarea>
        <textarea name="shipping_notes" class="form-control" placeholder="Shipping notes"></textarea>
        <input name="lead_time" type="number" class="form-control" placeholder="Lead Time (weeks)">
        <div class="form-control-files"></div>
        <button type="submit" class="btn btn-save">Save</button>
        <button type="button" class="btn btn-cancel" data-dismiss="modal">Cancel</button>
    </form>
</div>
@require('./partials/base-dialog-end-wrapper.hbs')
