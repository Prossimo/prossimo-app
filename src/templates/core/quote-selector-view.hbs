<select name="quote-selector-dropdown" id="quote-selector-dropdown" class="selectpicker show-tick" title="Choose a quote"{{#if is_disabled }} disabled{{/if}}>
    {{#each quote_list}}
        <option
            {{#if is_selected }}selected{{/if}}
            value="{{ id }}"
            data-content="<span class='label label-dropdown'><em>#</em>{{ id }}</span><span class='title'>{{ quote_name }}</span>"
        >
            {{ quote_name }}
    </option>
    {{/each}}
</select>
