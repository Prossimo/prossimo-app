<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Quote Info</h3>
    </div>
    <div id="quote-info-content" class="panel-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label class="col-sm-4">Quote ID</label>
                <div class="col-sm-8">{{{ id }}}</div>
            </div>
            <div class="form-group">
                <label class="col-sm-4">Quote Name</label>
                <div class="col-sm-8 quote-name-container">{{{ name }}}</div>
            </div>
            <div class="form-group">
                <label class="col-sm-4">Revision</label>
                <div class="col-sm-8 quote-revision-container"></div>
            </div>
            <div class="form-group">
                <label class="col-sm-4">Date</label>
                <div class="col-sm-8 quote-date-container"></div>
            </div>
        </div>
    </div>
</div>
